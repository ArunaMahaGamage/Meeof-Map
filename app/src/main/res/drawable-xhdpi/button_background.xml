<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#005e7974" />
            <gradient android:angle="-90" android:startColor="#00345953" android:endColor="#00689a92"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#90A3B1" />
            <solid android:color="#90A3B1"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#90A3B1" />
            <gradient android:angle="-90" android:startColor="#008dbab3" android:endColor="#0058857e" />
        </shape>
    </item>
</selector>